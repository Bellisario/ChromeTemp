{
    "name": "ChromeTemp Manager",
    "description": "NEVER REMOVE THIS EXTENSION: is the core of ChromeTemp that controls Chrome.",
    "version": "1.0",
    "permissions": [
        "tabs",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": ""
    },
    "manifest_version": 2,
    "icons": {
        "128": "chrome-temp.png"
    }
}
